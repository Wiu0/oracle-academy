
public class SalaAulaTeste {

	
	public static void main(String[] args) {
		SalaAula sa = new SalaAula();
		
		Aluno aluno = new Aluno();
		aluno.nome = "Wiu";
			
		sa.numeroSala = 1;
		sa.quantidadeMaxAluno = 100;
		sa.abc = aluno;
		
		System.out.println(sa.abc.nome);
		System.out.println(sa.abc.getNota1());
		System.out.println(sa.quantidadeMaxAlunoNoturno());
		System.out.println(sa.abc.nome);
		System.out.println(sa.abc.getNota1());
	}
}
