
public class SalaTeste {

	public static void main(String[] args) {
		Aluno al1 = new Aluno();
		al1.nome = "Wiu";
		al1.nota1 = 5;
		al1.nota2 = 7;
		al1.nota3 = 10;
		
		Sala s = new Sala();
		s.numero = 1;
		s.numeroCarteiras = 3;
		s.al = al1;
		
		
		System.out.println("nota 1:" + s.al.nota1);
		System.out.println("Quantidade de alunos na sala: " + s.pegarQuantidadeAlunos());

		
	}
}

