import java.util.Scanner;

public class MediaAlunoMetodo {

	public static void main(String[] args) {
		//Nota deve ser maior/igual 0 e menor/igual 10
		
		String nome = "Wiu";
		Scanner sc = new Scanner(System.in);
		double nota1 = sc.nextDouble(), nota2 = sc.nextDouble(), nota3 = sc.nextDouble(), nota4 = sc.nextDouble();
		double media = (nota1 + nota2 + nota3 + nota4)/4;
		
		if(nota1 >= 0 && nota1 <= 10) {
			System.out.println("Nota 1 valida");
		}else {
			System.out.println("Nota 1 invalida");
		}
		if(nota2 >= 0 && nota2 <= 10) {
			System.out.println("Nota 2 valida");
		}else {
			System.out.println("Nota 2 invalida");
		}
		if(nota3 >= 0 && nota3 <= 10) {
			System.out.println("Nota 3 valida");
		}else {
			System.out.println("Nota 3 invalida");
		}
		if(nota4 >= 0 && nota4 <= 10) {
			System.out.println("Nota 4 valida");
		}else {
			System.out.println("Nota 4 invalida");
		}
		
		
		
		System.out.println(nome + " tem mÃ©dia de " + media);
		sc.close();
		
		
	}
}
