import javax.swing.JOptionPane;

public class AlunoTeste {

	public static void main(String[] args) {
		Aluno al = new Aluno();
		al.nome = "Wiu";
		al.nota1 = 5;
		al.nota2 = 10;
		al.nota3 = 9;
		
		System.out.println("a media " + al.calcularMedia());
	
		al.nome = "Maria";
		al.nota1 = 10;
		al.nota2 = 9;
		al.nota3 = 5;
		
		JOptionPane.showMessageDialog(null, "A media " + al.calcularMedia());
	}
}
