import java.util.Scanner;

public class ListaNota {

	//Array
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Informe a quantidade de notas");
		int quantidadeNotas = Integer.parseInt(sc.nextLine());
		
		double notasAluno[] = new double[quantidadeNotas];
		
		for(int i = 0; i < quantidadeNotas; i++) {
			System.out.println("Informe o valor da nota");
			notasAluno[i] = sc.nextDouble();
		}
		
		
		System.out.println("Primeira nota informada foi: " + notasAluno[8]);
		sc.close();
	}
}
