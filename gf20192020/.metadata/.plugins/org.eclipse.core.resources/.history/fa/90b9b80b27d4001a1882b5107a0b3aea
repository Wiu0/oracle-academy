
public class AlunoTeste {

	public static void main(String[] args) {
		
		Aluno al = new Aluno();
		al.nome = "Wiu";
		al.nota1 = 5;
		al.nota2 = 7;
		al.nota3 = 10;
		
		al.calcularMedia();
		
		Aluno al1 = new Aluno();
		al1.setNome("Maria");
		al1.nota1 = 8;
		al1.nota2 = 6;
		al1.nota3 = 4;
		
		al1.calcularMedia();
	}
}
